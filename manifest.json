{
  "manifest_version":2,

  "name":"newtab",
  "description": "This extension lets you open each link in new tab",
  "version":"1.0.0",
  "author": "ritik saxena",

  "browser_action":{
    "default_icon":"dark.png",
    "default_title": "newTab"
  },
  "background":{
    "persistent":true,
    "scripts":["background.js"]
  },
  "content_scripts":[{
    "js":["script.js"],
    "matches":["<all_urls>"]
    }],
    "permissions":[
      "tabs",
      "storage"
    ]
}
